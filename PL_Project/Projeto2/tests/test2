def main
    declarations
        int n;
        int num;
        int min;
    end
    instructions
        print "Introduza um inteiro N:";

        input n;

        if(n>0) then
            print "Introduza N numeros:";

            input min;
            n = n - 1;

            if(n>0) then
                repeat
                    input num;
                    if(num<min) then
                        min = num;
                    end
                    n = n - 1;
                until (n==0) end
            end

            print "Numero menor:";
            print min;
        end
    end
end